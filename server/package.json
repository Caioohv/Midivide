{
  "name": "server",
  "version": "1.0.0",
  "description": "Backend do projeto midivide",
  "main": "index.js",
  "scripts": {
    "dev": "clear && nodemon index.js",
		"db-setup": "npx sequelize-cli db:create && npx sequelize-cli db:migrate && clear",
    "db-reset": "npx sequelize-cli db:drop && npm run db-setup",
		"db-seed": "npx sequelize-cli db:seed:all",
		"all": "npm run db-reset && npm run db-seed && npm run dev"
  },
  "author": "caio vieira",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.3.1",
    "nodemailer": "^6.9.2",
    "sequelize": "^6.31.1"
  },
  "devDependencies": {
    "eslint": "^8.40.0",
    "nodemon": "^2.0.22",
    "sequelize-cli": "^6.6.0"
  }
}
